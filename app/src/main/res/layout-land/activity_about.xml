<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.user0308.scutkicking.activity.AboutActivity">
<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/aboutview"
    android:background="@drawable/guanyuxin"
    />
    <ImageButton
        android:layout_width="220dp"
        android:layout_height="100dp"
        android:id="@+id/aboutreturn"
        android:background="@drawable/guanyufanhui"
        app:layout_constraintBottom_toBottomOf="@+id/aboutview"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/aboutview"
        app:layout_constraintTop_toTopOf="@+id/aboutview"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/aboutview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />


</android.support.constraint.ConstraintLayout>
